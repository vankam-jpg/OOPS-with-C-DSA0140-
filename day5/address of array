#include<iostream>
#include<bits/stdc++.h>
using namespace std;

int main()
{
 
 int i, size;
 cout<<"Enter The Size of The Array: ";
 cin>>size;
 
    int array[size];
 cout << "Enter The Elements Of An array";
 for (i = 0; i < size; i++) 
 {
        cin>>array[i];    
 }
 for (i = 0; i < size; i++) 
 {
        cout << "Address Of " << array[i]<<"is:" << &array[i]<<endl;    
 }
    return 0;
}
