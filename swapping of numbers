#include<iostream>
using namespace std;
void swap(int,int);
int main()
{
	int a,b;
	cout<<"enter the first value";
	cin>>a;
	cout<<"enter the second value";
	cin>>b;
	cout<<"\n before swapping the values are"<<a<<"and"<<b;
	swap(a,b);

}
void swap(int a,int b)
{
	int c;
	c=a;
	a=b;
	b=c;
	cout<<"\n after swapping the values are"<<a<<"and"<<b;
}
