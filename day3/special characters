
#include <iostream>
using namespace std;

void Check(char ch)
{
	if ((ch >= 65 && ch <= 90)
		|| (ch >= 97 && ch <= 122))
		cout << " Alphabet ";

	else if (ch >= 48 && ch <= 57)
		cout << " Digit ";
	else
		cout << " Special Character ";
}
int main()
{
	char ch;
	cout<<"enter the character:";
	cin>>ch;
	Check(ch);
	return 0;
}
